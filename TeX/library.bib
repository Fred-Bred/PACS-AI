   title = {How to Reduce the Effect of Framing on Messages About Health},
   volume = {25},
   url = {http://pubdb3.census.gov/macro/032008/},
   year = {2010},
}
@article{Burger2009,
   abstract = {The author conducted a partial replication of
Stanley Milgram's (1963
