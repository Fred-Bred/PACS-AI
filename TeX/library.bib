@article{Rose1988,
   abstract = {It is difficult to be precise about just how the emergence of the psychological sciences in the nineteenth century was linked up to other political and social events. This difficulty is compounded by the problem of grasping what actually does differentiate these sciences from those religious, philosophical, and medical discourses on human mental life which preceded them. In this paper I review some different ways of looking at these two linked questions and indicate some of their shortcomings. I suggest an alternative perspective on the history of the psychological sciences and their relations with some more general social transformations which commenced in the mid-nineteenth century. Lastly I will say a little about one specific, and I think significant, way in which the psychological sciences have tranformed our intellectual and practical universe.' I The psychological sciences-psychology, psychiatry, and the other disciplines which designate themselves with the prefix 'psy-'-are certainly not devoid of an historical consciousness. There are many weighty tomes which tell the story of the long development of the scientific study of psychological functioning-normal and pathological.z Almost every psychiatric or psychological textbook appears obliged to include an historical chapter or review, however desultory, of the topics under discussion. These texts repeatedly tell us the story of the development of the psychological sciences in similar terms: they have a long past but a short history. A long past: a continuous tradition of speculation concerning the nature, vicissitudes, and pathologies of the human soul, virtually coextensive with the human intellect},
   author = {Nikolas Rose},
   issue = {2},
   journal = {History of the Human Sciences},
   title = {Calculable minds and manageable individuals},
   volume = {1},
   year = {1988},
}
