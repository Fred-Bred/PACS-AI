@inbook{Campbell2017,
   abstract = {Abstract Secularism is on the rise within the United States. Religious nonaffiliation has increased dramatically, while other measures of religiosity?worship attendance, belief in God, belief in scripture?are waning. This essay considers the political implications of this secular turn, in light of the historically high rates of religiosity within the United States. In doing so, we distinguish between passive and active secularism and describe measures of each. Passive secularism refers to not being religious?not identifying with a religion, not attending religious services, and not believing in God. Active secularism is the affirmative adoption of a secular worldview and identity. With this distinction, we can trace Americans' growing political polarization along religious?secular lines. One form of passive secularism?religious nonaffiliation?is often triggered by a backlash to the Religious Right. More generally, both passively and actively secular voters are moving to the political left. Furthermore, while smaller in number than passive secularists, active secularists are highly engaged in politics, comprising a sizeable share of the Democratic Party's activist base.},
   author = {David E. Campbell and Geoffrey C. Layman},
   doi = {10.1002/9781118900772.etrds0423},
   journal = {Emerging Trends in the Social and Behavioral Sciences},
   month = {11},
   pages = {1-13},
   publisher = {Wiley},
   title = {The Politics of Secularism in the United States},
   year = {2017},
}
@misc{,
   abstract = {Across Europe, climate citizens' assemblies (CCA) are introduced to reinvigorate democracy and to aid governments in securing support for green transition policies. For supporters, CCAs are ways to turn the current climate and biodiversity crises into an opportunity for democratic renewal. Critics, however, worry that the CCAs are unable to exert any real influence and thus make it harder (not easier) to develop and secure a more sustainable future for all. To address this conundrum, this article proposes a new conceptualization of the transmission mechanisms between CCAs and their political systems, which then informs a comparative analysis of two national climate assemblies: Scotland and Denmark. The first part of the article outlines the main theoretical debates about CCAs and suggests a new analytical framework for analyzing transmission mechanisms. The second part of the article dives into the two cases and how their divergent transmission mechanisms have affected the role and impact of the assembly.},
   author = {Ingrid Helene Brandt Jensen and Frederik Langkjaer and Lars Tønder},
   title = {Climate citizens' assemblies and their political systems: a comparative analysis of transmission mechanisms in action},
   year = {2024},
}
