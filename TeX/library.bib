@article{Evans2013,
   abstract = {Dual-process and dual-system theories in both cognitive and social psychology have been subjected to a number of recently published criticisms. However, they have been attacked as a category, incorrectly assuming there is a generic version that applies to all. We identify and respond to 5 main lines of argument made by such critics. We agree that some of these arguments have force against some of the theories in the literature but believe them to be overstated. We argue that the dual-processing distinction is supported by much recent evidence in cognitive science. Our preferred theoretical approach is one in which rapid autonomous processes (Type 1) are assumed to yield default responses unless intervened on by distinctive higher order reasoning processes (Type 2). What defines the difference is that Type 2 processing supports hypothetical thinking and load heavily on working memory. © The Author(s) 2013.},
   author = {Jonathan St B.T. Evans and Keith E. Stanovich},
   doi = {10.1177/1745691612460685},
   issn = {17456916},
   issue = {3},
   journal = {Perspectives on Psychological Science},
   keywords = {dual processes,dual systems,individual differences,rationality,working memory},
   month = {5},
